<!-- Introduction Classes -->

<owl:Class rdf:ID="First-Meeting">
  <rdfs:subClassOf rdf:resource="&ome;Introduction" />
  <rdfs:comment>This class represents an event focused on the first meeting of two or more characters</rdfs:comment>
  <rdfs:label>First Meeting</rdfs:label>
</owl:Class>

